<%-include("navbar.ejs"); %>
<div class="row">
    <div class="col-12">
    <p class="h3 mb-3"><strong>Booking</strong> List</p>
    <table class="table table-striped table-hover mt-3 ">

        <tr>
            <th> Booking Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Date And Time</th>
            <th>destination</th>
            <th>Message</th>
            <th class="fw-bold">Status</th>
            <th>Action</th>
        </tr>
        <% booking.map(function(row, i) { %>
            <tr>
                <td><%= i+1 %></td>
                <td><%= row.name %></td>
                <td><%= row.user_name %></td>
                <td><%= row.date_time %></td>
                <td><%= row.destination %></td>
                <td><%= row.special_request %></td>
                <td class="text-success fw-bold"><%= row.booking_status %></td>
                <td>
                   
                    <a href="/admin/accepted_delete/<%= row.id %>" class="btn btn-danger">Delete</a>
                </td>
            </tr>
        <% }); %>
    </table>    
    </div>
</div>
<%-include("footer.ejs"); %>